<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="40dp"
                android:height="40dp" />
            <padding
                android:bottom="40dp"
                android:left="40dp"
                android:right="40dp"
                android:top="40dp" />
            <stroke
                android:width="15dp"
                android:color="#ffffff" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size
                android:width="50dp"
                android:height="50dp" />
            <solid android:color="@color/white" />

        </shape>
    </item>

</layer-list>